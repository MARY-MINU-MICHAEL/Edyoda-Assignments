        "import string\n",
        "print(\"Alphabet from a-z:\")\n",
        "l=[]\n",
        "for letter in string.ascii_lowercase:\n",
        "  l.append(letter)\n",
        "print(l)\n",
        "d={}\n",
        "for i in l:\n",
        "  d[i]=ord(i)\n",
        "print(d)"
